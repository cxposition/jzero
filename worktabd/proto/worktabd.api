syntax = "v1"

type Request {
	Name string `path:"name,options=you|me"`
}

type Response {
	Message string `json:"message"`
}

@server (
	prefix: /api/v1
	group:  credentialapi
)
service worktabd {
	@handler CredentialNameHandler
	get /credential/:name (Request) returns (Response)
}

@server (
	prefix: /api/v1
	group:  machineapi
)
service worktabd {
	@handler MachineNameHandler
	get /machine/:name (Request) returns (Response)
}

